digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	user [label=user color=red fillcolor=darksalmon peripheries=2]
	intro [label=intro]
	hero [label=hero]
	summary [label=summary]
	feature [label=feature]
	newbie [label=newbie]
	easy_hero [label=easy_hero]
	location [label=location]
	show_easy_hero [label=show_easy_hero]
	show_location [label=show_location]
	get_location [label=get_location]
	free [label=free]
	hero_intro [label=hero_intro]
	user -> intro [label="advance [is_going_to_intro]"]
	user -> hero [label="advance [is_going_to_hero]"]
	intro -> summary [label="advance [is_going_to_summary]"]
	intro -> feature [label="advance [is_going_to_feature]"]
	intro -> newbie [label="advance [is_going_to_newbie]"]
	intro -> user [label="advance [is_going_to_user] | go_back"]
	summary -> intro [label="advance [is_going_to_intro]"]
	summary -> user [label="advance [is_going_to_user]"]
	feature -> intro [label="advance [is_going_to_intro]"]
	feature -> user [label="advance [is_going_to_user]"]
	newbie -> intro [label="advance [is_going_to_intro]"]
	newbie -> easy_hero [label="advance [is_going_to_easy_hero]"]
	newbie -> location [label="advance [is_going_to_location]"]
	newbie -> user [label="advance [is_going_to_user]"]
	location -> newbie [label="advance [is_going_to_newbie]"]
	location -> show_location [label="advance [is_going_to_show_location]"]
	location -> user [label="advance [is_going_to_user]"]
	easy_hero -> newbie [label="advance [is_going_to_newbie]"]
	easy_hero -> show_easy_hero [label="advance [is_going_to_show_easy_hero]"]
	easy_hero -> user [label="advance [is_going_to_user]"]
	show_easy_hero -> easy_hero [label="advance [back_show_easy_hero]"]
	show_easy_hero -> user [label="advance [is_going_to_user]"]
	show_location -> get_location [label="advance [is_going_to_get_location]"]
	show_location -> location [label="advance [is_going_to_location]"]
	show_location -> user [label="advance [is_going_to_user]"]
	get_location -> show_location [label="advance [back_show_location]"]
	get_location -> user [label="advance [is_going_to_user]"]
	hero -> free [label="advance [is_going_to_free]"]
	hero -> hero_intro [label="advance [is_going_to_hero_intro]"]
	hero -> user [label="advance [is_going_to_user] | go_back"]
	free -> user [label="advance [is_going_to_user]"]
	hero_intro -> user [label="advance [is_going_to_user]"]
}
